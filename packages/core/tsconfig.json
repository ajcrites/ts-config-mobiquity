{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "es7"]
  },
  "atom": {
    "rewriteTsconfig": false
  }
}
