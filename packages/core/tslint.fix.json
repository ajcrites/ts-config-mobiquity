{
  "extends": "ts-config-mobiquity-core/tslint.json",
  "rules": {
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "groups": [
          { "name": "relative paths", "match": "^\\.\\.?", "order": 30 },
          { "name": "project imports", "match": "^src\/", "order": 20 },
          { "name": "libraries", "match": ".*", "order": 10 }
        ]
      }
    ]
  }
}
